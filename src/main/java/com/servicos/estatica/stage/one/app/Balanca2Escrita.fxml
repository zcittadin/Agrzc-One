<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="151.0" minWidth="320.0" prefHeight="151.0" prefWidth="320.0" styleClass="mainGradient" stylesheets="@../style/style.css" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.servicos.estatica.stage.one.controller.Balanca2EscritaController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Informe o valor de pesagem:" textFill="WHITE">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="txtValor" layoutX="24.0" layoutY="51.0" prefHeight="30.0" prefWidth="277.0">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Button fx:id="btCancel" layoutX="168.0" layoutY="107.0" mnemonicParsing="false" onAction="#cancel" prefHeight="30.0" prefWidth="99.0" styleClass="btCancel" stylesheets="@../css/style.css" text="Cancela">
         <font>
            <Font size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="btOk" layoutX="59.0" layoutY="107.0" mnemonicParsing="false" onAction="#saveValorPesagem" prefHeight="30.0" prefWidth="99.0" styleClass="btOk" stylesheets="@../css/style.css" text="Ok">
         <font>
            <Font size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
